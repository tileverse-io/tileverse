message spark_schema {
  optional binary id (STRING);
  optional binary geometry;
  required group bbox {
    optional float xmin;
    optional float xmax;
    optional float ymin;
    optional float ymax;
  }
  required int32 version;
  optional group sources (LIST) {
    repeated group list {
      optional group element {
        optional binary property (STRING);
        optional binary dataset (STRING);
        optional binary record_id (STRING);
        optional binary update_time (STRING);
        optional double confidence;
      }
    }
  }
  optional group names {
    optional binary primary (STRING);
    optional group common (MAP) {
      repeated group key_value {
        required binary key (STRING);
        optional binary value (STRING);
      }
    }
    optional group rules (LIST) {
      repeated group list {
        optional group element {
          optional binary variant (STRING);
          optional binary language (STRING);
          optional binary value (STRING);
          optional group between (LIST) {
            repeated group list {
              optional double element;
            }
          }
          optional binary side (STRING);
        }
      }
    }
  }
  optional group categories {
    optional binary primary (STRING);
    optional group alternate (LIST) {
      repeated group list {
        optional binary element (STRING);
      }
    }
  }
  optional double confidence;
  optional group websites (LIST) {
    repeated group list {
      optional binary element (STRING);
    }
  }
  optional group socials (LIST) {
    repeated group list {
      optional binary element (STRING);
    }
  }
  optional group emails (LIST) {
    repeated group list {
      optional binary element (STRING);
    }
  }
  optional group phones (LIST) {
    repeated group list {
      optional binary element (STRING);
    }
  }
  optional group brand {
    optional binary wikidata (STRING);
    optional group names {
      optional binary primary (STRING);
      optional group common (MAP) {
        repeated group key_value {
          required binary key (STRING);
          optional binary value (STRING);
        }
      }
      optional group rules (LIST) {
        repeated group list {
          optional group element {
            optional binary variant (STRING);
            optional binary language (STRING);
            optional binary value (STRING);
            optional group between (LIST) {
              repeated group list {
                optional double element;
              }
            }
            optional binary side (STRING);
          }
        }
      }
    }
  }
  optional group addresses (LIST) {
    repeated group list {
      optional group element {
        optional binary freeform (STRING);
        optional binary locality (STRING);
        optional binary postcode (STRING);
        optional binary region (STRING);
        optional binary country (STRING);
      }
    }
  }
}
